<Window x:Class="TestWpfNameOf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestWpfNameOf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" d:DataContext="{d:DesignInstance {x:Type local:MainWindowViewModel}}">
    <Grid Margin="0,0,232,109">
        <Button x:Name="button1" Content="Set TexBlock" HorizontalAlignment="Left" Margin="26,32,0,0" VerticalAlignment="Top" Width="123" RenderTransformOrigin="-2.178,0.764" Height="68" Click="button1_Click"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="26,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="46" Width="144" Text="{Binding TextBlockData}"/>

    </Grid>
</Window>
